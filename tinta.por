programa 
{

  funcao inicio()
  {

      real largura, altura, area, tinta

      escreva("Largura: ")
      leia (largura) 

      escreva("Altura: ")
      leia(altura)

      area=altura * largura

      tinta=area / 2

      escreva("A area é igual á: ", area ) 
      escreva("\nA quantidade de tinta para pintar a area sera: ", tinta)

  }
  
}     